// implementation of a set using a closure
let set = func(list) {
    let store = {};
    if (type(list) == "list") {
        for (let i = 0; i < 3; i = i + 1) {
            let key = list[i];
            store[key] = true;
        }
    }
    return {
        "add": func(x) { store[x] = true; },
        "has": func(x) { return store[x] == true }
    }
};
let nums = set([1, 2, 3]);
nums.set(4);
log(nums.has(4)); // true