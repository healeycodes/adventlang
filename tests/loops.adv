let count = 0;
for (let i = 0; i < 10; i = i + 1) {
    count = count + 1;
}
assert(count, 10);

for (let i = 0; i < 1; i = i + 1) {
    continue
    // Shouldn't execute
    assert(true, false);
}
assert(count, 10);

for (;;) {
    break
    // Shouldn't execute
    assert(true, false);
}

let ran_once = 0;
while (true) {
    ran_once = ran_once + 1;
    break
}
assert(ran_once, 1);

for (let i = 0; i < 10; i = i + 1) {
    if (true) {
        // Break inside nested block
        break
    }
    assert(true, false);
}

for (let i = 0; i < 10; i = i + 1) {
    if (true) {
        // Continue inside nested block
        continue
    }
    assert(true, false);
}
